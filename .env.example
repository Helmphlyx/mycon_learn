# MyCon Learn Configuration
# Copy this to .env and customize

# Database (SQLite by default, can use PostgreSQL for production)
DATABASE_URL=sqlite:///./mycon_learn.db

# Authentication (set a password to enable login protection)
APP_PASSWORD=your-secret-password-here

# Server settings
HOST=0.0.0.0
PORT=8000
DEBUG=false

# CORS (comma-separated origins, or * for all)
CORS_ORIGINS=*
